<!-- PartCommand -->
<string name="part_command_description">Leave a channel</string>
<string name="part_command_usage">/part [#channel] [message]</string>
<string name="part_error_no_channel">You must be in a channel or specify one</string>
<string name="part_success">Leaving %1$s</string>
<string name="part_success_with_message">Leaving %1$s (%2$s)</string>
<string name="part_error_general">Error leaving channel: %1$s</string>

<!-- KickCommand -->
<string name="kick_command_description">Kick a user from the channel</string>
<string name="kick_command_usage">/kick &lt;nick&gt; [reason] or /kick #channel &lt;nick&gt; [reason]</string>
<string name="kick_error_no_nick">You must specify a nickname</string>
<string name="kick_error_no_channel">You must be in a channel or specify one</string>
<string name="kick_error_usage">Usage: %1$s</string>
<string name="kick_success">%1$s kicked from %2$s</string>
<string name="kick_success_with_reason">%1$s kicked from %2$s (Reason: %3$s)</string>
<string name="kick_error_general">Error kicking user: %1$s</string>

<!-- BanCommand -->
<string name="ban_command_description">Ban a user from the channel</string>
<string name="ban_command_usage">/ban &lt;nick|hostmask&gt; or /ban #channel &lt;hostmask&gt;</string>
<string name="ban_error_no_hostmask">You must specify a nickname or hostmask</string>
<string name="ban_error_no_channel">You must be in a channel or specify one</string>
<string name="ban_error_usage">Usage: %1$s</string>
<string name="ban_success">%1$s banned from %2$s</string>
<string name="ban_error_general">Error banning user: %1$s</string>

<!-- UnbanCommand -->
<string name="unban_command_description">Unban a user from the channel</string>
<string name="unban_command_usage">/unban &lt;nick|hostmask&gt; or /unban #channel &lt;hostmask&gt;</string>
<string name="unban_error_no_hostmask">You must specify a nickname or hostmask</string>
<string name="unban_error_no_channel">You must be in a channel or specify one</string>
<string name="unban_error_usage">Usage: %1$s</string>
<string name="unban_success">%1$s unbanned from %2$s</string>
<string name="unban_error_general">Error unbanning user: %1$s</string>

<!-- InviteCommand -->
<string name="invite_command_description">Invite a user to a channel</string>
<string name="invite_command_usage">/invite &lt;nick&gt; [#channel]</string>
<string name="invite_error_no_nick">You must specify a nickname</string>
<string name="invite_error_no_channel">You must be in a channel or specify one</string>
<string name="invite_error_invalid_channel">Channel name must start with #</string>
<string name="invite_error_usage">Usage: %1$s</string>
<string name="invite_success">%1$s invited to %2$s</string>
<string name="invite_error_general">Error inviting user: %1$s</string>
